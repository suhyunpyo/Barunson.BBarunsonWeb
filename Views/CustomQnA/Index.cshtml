        <li class="breadcrumb-item"><a href="#">고객센터</a></li>
        <li class="breadcrumb-item active">고객QnA 관리</li>
    </ol>
</nav>

<form class="mb-9" method="get" asp-action="Index" asp-controller="CustomQna" id="frmSearch">
      <div class="row justify-content-between align-items-end g-3 mb-5">
        <div class="col-12 col-sm-auto col-xl-8">
            <h2 class="mb-0">고객QnA 관리</h2>
        </div>

    </div>

<input class="form-check-input"  type="checkbox" id="chkBrandAll" value="ALL" />
<label for="chkBrandAll" class="form-check-input" >전체</label>
@{
    
}
@for (var i = 0; i < Model.ProductBrands.Count; i++)
{
    <input  class="form-check-input" type="checkbox" asp-for="@Model.ProductBrands[i].Selected" />
    <label class="form-check-label" asp-for="@Model.ProductBrands[i].Selected">@Model.ProductBrands[i].Text</label>
    <input type="hidden" asp-for="@Model.ProductBrands[i].Value" />
    <input type="hidden" asp-for="@Model.ProductBrands[i].Text" />
}

    <div class="row gx-3 gy-4">
        <div class="col-sm-6 col-md-1">
            <div class="form-floating">
                <select class="form-select" asp-for="Status" asp-items="Model.Statuss"></select>
                <label for="PChasu">진행상황</label>
            </div>
        </div>

