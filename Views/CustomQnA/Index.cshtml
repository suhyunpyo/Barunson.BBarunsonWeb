<div class="input-group mb-3">

    <input class="form-control" type="text" asp-for="KeyWord" placeholder="검색어를 입력하세요" aria-label="Username" aria-describedby="addon-wrapping" />
</div>

<div class="col-sm-6 col-md-3">
    <div class="d-flex" style="width:70%;height: 100%;">
        <button class="btn btn-primary px-5 me-2" style="font-size:1rem">검색</button>
    </div>
</div>
</div>
